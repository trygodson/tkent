{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal"
    },
    "quantity": {
      "type": "integer"
    },
    "types": {
      "type": "json"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "inversedBy": "products"
    }
  }
}
